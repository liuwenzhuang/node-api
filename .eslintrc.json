{
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "plugin:import/errors",
    "prettier"
  ],
  "plugins": ["prettier", "jest"],
  "rules": {
    // 0: off 1:warn 2:error
    "no-console": 1,
    "prettier/prettier": 2
  },
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "settings": {}
}
